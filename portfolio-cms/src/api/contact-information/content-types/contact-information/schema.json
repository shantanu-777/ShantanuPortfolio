    },
    "phone": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "socialLinks": {
      "type": "component",
      "repeatable": true,
      "component": "shared.social-link"
    },
    "contactMessage": {
      "type": "text"
    }
  }
}


